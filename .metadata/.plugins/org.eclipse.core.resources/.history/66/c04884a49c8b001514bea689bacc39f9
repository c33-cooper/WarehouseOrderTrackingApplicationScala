/**
 * Imports
 */
import scalafx.beans.property.ObjectProperty

/**
 * @author callum
 * @description CustomerOrderEntity.scala is an entity class
 *              for holding attributes for the customer order
 */
class CustomerOrderEntity(val idOrders : Int, val orderDateCreated : String,
                          val orderVerified : Boolean, val orderTotal : Double,
                          val productQuantity : Int, val orderStatus : String){
  
  // Entity attributes
  val idOrders = new ObjectProperty[Int](this, "", idOrders)
}